<div id="<%= dom_id branch %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= branch.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Code:</strong>
    <%= branch.code %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Ismain:</strong>
    <%= branch.isMain? ? "Yes" : "No" %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= branch.status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Contact:</strong>
    <%= branch.contact_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Address:</strong>
    <%= branch.address_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Business:</strong>
    <%= branch.business_id %>
  </div>
</div>
