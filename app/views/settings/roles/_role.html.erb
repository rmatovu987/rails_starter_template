<div id="<%= dom_id role %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Business:</strong>
    <%= role.business_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= role.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= role.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= role.status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Unique:</strong>
    <%= role.unique_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Encoded key:</strong>
    <%= role.encoded_key %>
  </div>
</div>
