<div id="<%= dom_id user %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Firstname:</strong>
    <%= user.firstname %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Lastname:</strong>
    <%= user.lastname %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Email:</strong>
    <%= user.email %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= user.status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Unique:</strong>
    <%= user.unique_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Encoded key:</strong>
    <%= user.encoded_key %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Super user:</strong>
    <%= user.super_user? ? "Yes" : "No" %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Contact:</strong>
    <%= user.contact_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Address:</strong>
    <%= user.address_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Business:</strong>
    <%= user.business_id %>
  </div>
</div>
