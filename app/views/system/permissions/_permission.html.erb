<div id="<%= dom_id permission %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Business:</strong>
    <%= permission.business_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= permission.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= permission.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Unique:</strong>
    <%= permission.unique_id %>
  </div>
